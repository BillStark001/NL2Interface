{
  "name": "chatui-react",
  "version": "1.0.0",
  "description": "chatui re-implementation in react. Including a dedicated server written in expressjs.",
  "scripts": {
    "preinstall": "npm install -g eslint@7.6.0",
    "client-start": "cd client && react-scripts start",
    "client-build": "cd client && react-scripts build && mv build ../build",
    "start": "npm run lint:express && npm run lint:react && concurrently \"npm run start:dev\" \"npm run client-start\" --disable-host-check",
    "start-skip-lint": "concurrently \"npm run start:dev\" \"npm run client-start\" --disable-host-check",
    "start:dev": "cross-env NODE_ENV=development nodemon server/index.js --ignore client/ --ignore admin/ --disable-host-check",
    "start:prod": "cross-env NODE_ENV=production node server/index.js --ignore client/ --ignore admin/  --disable-host-check",
    "lint:react": "eslint ./client/src/**/*.tsx",
    "lint:express": "eslint ./server/**/*.js"
  },
  "author": "Ray",
  "license": "ISC",
  "devDependencies": {
    "@types/reactstrap": "^8.5.1",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "cross-env": "^7.0.2",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/sql-formatter": "^2.3.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "chalk": "^4.1.0",
    "concurrently": "^5.2.0",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "csv-parse": "^4.11.1",
    "csv-parser": "^2.3.3",
    "dayjs": "^1.8.31",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "html-webpack-plugin": "^4.3.0",
    "lodash": "^4.17.19",
    "mobx": "^5.15.5",
    "mobx-react": "^6.2.5",
    "node-fetch": "^2.6.0",
    "openai": "^3.0.0",
    "pg": "^8.3.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-data-grid": "7.0.0-canary.16",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.2",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-social-login-buttons": "^3.1.0",
    "react-syntax-highlighter": "^12.2.1",
    "react-virtualized": "^9.21.2",
